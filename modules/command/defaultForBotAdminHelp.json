    },
    {
      "name": "Wikipedia Search",
      "triggers": ["wikiSearch","wikisearch", "wiki"],
      "function": "wikiSearch",
      "helptext": "<b>/wikiSearch</b>",
      "description": "Finds top 3 results from Wikipedia",
