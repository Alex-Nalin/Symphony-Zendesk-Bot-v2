{
  "commands": [
    {
      "name": "atMentionRoom",
      "triggers": ["Room", "room"],
      "function": "atMentionRoom",
      "helptext": "<b>/room</b> MSG",
      "description": "@mentions all users of the current room and sends a message (MSG) ",
      "param":"None",
      "example":"/room Hi all",
      "category": "Miscellaneous",
      "scope": "Internal",
      "permission": "Bot Admin"
    },
    {
      "name": "ZDStream",
      "triggers": ["ZDStream", "ZDstream", "zdstream"],
      "function": "botStream",
      "helptext": "<b>/ZDStream</b> TYPE",
      "description": "This will return all the chat that users have with the Bot, this could be of any type (IM/MIM and ROOMs)",
      "param":"TYPE = 'IM', 'ROOM' or 'ALL'",
      "example":"/ZDStream im <br></br>/ZDStream room <b></b>/ZDStream all",
      "category": "Miscellaneous",
      "scope": "Internal",
      "permission": "Bot Admin"
    },
    {
      "name": "ZDBlast",
      "triggers": ["ZDBlast", "ZDblast", "zdblast"],
      "function": "botMessageBlast",
      "helptext": "<b>/ZDBlast</b> TYPE TXT",
      "description": "Sends a message to all IM/MIM or ROOMs or even all Types with a specific message",
      "param":"TYPE = 'IM', 'ROOM' or 'ALL' <br></br>TXT = text of message to blast",
      "example":"/ZDBlast IM The bot has been updated",
      "category": "Miscellaneous",
      "scope": "Internal",
      "permission": "Bot Admin"
    },
    {
      "name": "status",
      "triggers": ["status", "poke", "check", "test", "ping"],
      "function": "SendStatusCheck",
      "helptext": "<b>/status</b>",
      "description": "Returns the status of SymphonyZendeskBot. This command bypasses the Command Queue.",
      "param":"None",
      "example":"/ping",
      "category": "Miscellaneous",
      "scope": "External",
      "permission": "All",
      "immediate": true
    },
    {
      "name": "help",
      "triggers": ["help", "Help", "HELP"],
      "function": "SymphonyZendeskBotHelp",
      "helptext": "<b>/help</b>",
      "description": "Tells you what the bot can do.",
      "param":"none",
      "example":"/help",
      "scope": "External",
      "category": "Miscellaneous",
      "permission": "All"
    },
    {
      "name": "mention",
      "triggers": ["all", "here", "everyone", "room"],
      "function": "atMentionRoom",
      "helptext": "<b>/all</b>",
      "description": "@mentions all members of a stream/room to look at posted message",
      "param":"None",
      "example":"/all This is an important message bla bla",
      "category": "Miscellaneous",
      "scope": "Internal",
      "permission": "All",
      "immediate": true
    },
    {
      "name": "addAcronym",
      "triggers": ["addAcronym", "addacronym"],
      "function": "addAcronym",
      "helptext": "<b>/addAcronym</b>",
      "description": "Add acronym to the dictionary",
      "param":"acronym",
      "example":"/addAcronym VIP - Very Important Person",
      "category": "Create/update",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "removeAcronym",
      "triggers": ["removeAcronym","removeacronym", "remAcronym", "remacronym", "delAcronym", "delacronym", "DeleteAcronym"],
      "function": "removeAcronym",
      "helptext": "<b>/removeAcronym</b>",
      "description": "Removes the acronym from the dictionary",
      "param":"acronym",
      "example":"/removeAcronym VIP",
      "category": "Create/update",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "findAcronym",
      "triggers": ["findAcronym","findacronym", "FindAcronym", "Findacronym", "Acronym", "acronym", "meaning", "Meaning", "define", "Define"],
      "function": "findAcronym",
      "helptext": "<b>/findAcronym</b>",
      "description": "Finds existing acronym",
      "param":"acronym",
      "example":"/findAcronym VIP",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "listAllAcronyms",
      "triggers": ["listAllAcronyms", "listallacronyms", "listAcronyms", "listacronyms", "AllAcronyms", "allAcronyms", "allacronyms"],
      "function": "listAllAcronyms",
      "helptext": "<b>/listAllAcronyms</b>",
      "description": "List all the existing acronym",
      "param":"None",
      "example":"/listAllAcronyms",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "echo",
      "triggers": ["echo"],
      "function": "SendSymphonyEchoV2",
      "helptext": "<b>/echo</b>",
      "description": "Repeats whatever chat data is sent to the bot. Useful for testing the bot connection.",
      "param":"[Text]",
      "example":"/echo hello world",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List",
      "immediate": true
    },
    {
      "name": "translate",
      "triggers": ["Translate", "translate"],
      "function": "GetGoogleTranslation",
      "helptext": "<b>/translate</b>",
      "description": "Uses Google Translate to translate the search word or phrase.",
      "param":"Text",
      "example":"/translate bonjour",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "quote",
      "triggers": ["quote", "$", "qt"],
      "function": "GetAlphaVantageStockQuote",
      "helptext": "<b>/quote</b>",
      "description": "Pulls the most recent open and close prices for the specified ticker symbol from Alpha Vantage.",
      "param":"tickerSymbol",
      "example":"/quote $appl",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "gif",
      "triggers": ["gif", "giphy" ],
      "function": "GetGiphyImage",
      "helptext": "<b>/gif</b>",
      "description": "Searches and brings a funny gif from Giphy. If search text is blank this command will return a random GIF. (using Card)",
      "param":"Text (optional)",
      "example":"/gif Symphony",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "weather",
      "triggers": ["Weather", "weather"],
      "function": "weather",
      "helptext": "<b>/weather</b>",
      "description": "Gives the current weather for the given location",
      "param":"location + days to forecast",
      "example":"/weather london or /weather new_york",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "Quote of the Day",
      "triggers": ["quoteoftheday","qod", "QoD", "QOD"],
