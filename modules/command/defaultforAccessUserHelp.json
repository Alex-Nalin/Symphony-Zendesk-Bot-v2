{
  "commands": [
    {
      "name": "status",
      "triggers": ["status", "poke", "check", "test", "ping"],
      "function": "SendStatusCheck",
      "helptext": "<b>/status</b>",
      "description": "Returns the status of SymphonyZendeskBot. This command bypasses the Command Queue.",
      "param":"None",
      "example":"/ping",
      "category": "Miscellaneous",
      "scope": "External",
      "permission": "All",
      "immediate": true
    },
    {
      "name": "help",
      "triggers": ["help", "Help", "HELP"],
      "function": "SymphonyZendeskBotHelp",
      "helptext": "<b>/help</b>",
      "description": "Tells you what the bot can do.",
      "param":"none",
      "example":"/help",
      "scope": "External",
      "category": "Miscellaneous",
      "permission": "All"
    },
    {
      "name": "addAcronym",
      "triggers": ["addAcronym", "addacronym"],
      "function": "addAcronym",
      "helptext": "<b>/addAcronym</b>",
      "description": "Add acronym to the dictionary",
      "param":"acronym",
      "example":"/addAcronym VIP - Very Important Person",
      "category": "Create/update",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "removeAcronym",
      "triggers": ["removeAcronym","removeacronym", "remAcronym", "remacronym", "delAcronym", "delacronym", "DeleteAcronym"],
      "function": "removeAcronym",
      "helptext": "<b>/removeAcronym</b>",
      "description": "Removes the acronym from the dictionary",
      "param":"acronym",
      "example":"/removeAcronym VIP",
      "category": "Create/update",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "findAcronym",
      "triggers": ["findAcronym","findacronym", "FindAcronym", "Findacronym", "Acronym", "acronym", "meaning", "Meaning", "define", "Define"],
      "function": "findAcronym",
      "helptext": "<b>/findAcronym</b>",
      "description": "Finds existing acronym",
      "param":"acronym",
      "example":"/findAcronym VIP",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "listAllAcronyms",
      "triggers": ["listAllAcronyms", "listallacronyms", "listAcronyms", "listacronyms", "AllAcronyms", "allAcronyms", "allacronyms"],
      "function": "listAllAcronyms",
      "helptext": "<b>/listAllAcronyms</b>",
      "description": "List all the existing acronym",
      "param":"None",
      "example":"/listAllAcronyms",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "echo",
      "triggers": ["echo"],
      "function": "SendSymphonyEchoV2",
      "helptext": "<b>/echo</b>",
      "description": "Repeats whatever chat data is sent to the bot. Useful for testing the bot connection.",
      "param":"[Text]",
      "example":"/echo hello world",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List",
      "immediate": true
    },
    {
      "name": "translate",
      "triggers": ["Translate", "translate"],
      "function": "GetGoogleTranslation",
      "helptext": "<b>/translate</b>",
      "description": "Uses Google Translate to translate the search word or phrase.",
      "param":"Text",
      "example":"/translate bonjour",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "quote",
      "triggers": ["quote", "$", "qt"],
      "function": "GetAlphaVantageStockQuote",
      "helptext": "<b>/quote</b>",
      "description": "Pulls the most recent open and close prices for the specified ticker symbol from Alpha Vantage.",
      "param":"tickerSymbol",
      "example":"/quote $appl",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "gif",
      "triggers": ["gif", "giphy" ],
      "function": "GetGiphyImage",
      "helptext": "<b>/gif</b>",
      "description": "Searches and brings a funny gif from Giphy. If search text is blank this command will return a random GIF. (using Card)",
      "param":"Text (optional)",
      "example":"/gif Symphony",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "weather",
      "triggers": ["Weather", "weather", "Forecast", "forecast", "Cast", "cast"],
      "function": "weather",
      "helptext": "<b>/weather</b>",
      "description": "Gives the weather forecast for the given location from 1 to 7 days.",
      "param":"location + days to forecast",
      "example":"/weather london or /weather london 5",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "Quote of the Day",
      "triggers": ["quoteoftheday","qod", "QoD", "QOD"],
      "function": "QoD",
      "helptext": "<b>/qod</b>",
      "description": "Retrieves a Quote of the Day to ponder on. /qod .",
      "param":"None",
      "example":"/qod",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "Fun Quote",
      "triggers": ["funQuote", "funquote", "fun"],
      "function": "funQuote",
      "helptext": "<b>/funQuote</b>",
      "description": "Displays quotes from famous movies and person.",
      "param":"None",
      "example":"/funQuote",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "Joke",
      "triggers": ["Joke", "joke", "jokes"],
      "function": "joke",
      "helptext": "<b>/joke</b>",
      "description": "Displays random joke",
      "param":"None",
      "example":"/joke",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "name": "Wikipedia Search",
      "triggers": ["wikiSearch","wikisearch", "wiki"],
      "function": "wikiSearch",
      "helptext": "<b>/wikiSearch</b>",
      "description": "Finds top 3 results from Wikipedia",
      "param":"Search Term",
      "example":"/wikiSearch Symphony Communication",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "triggers": ["Whois", "whois"],
      "function": "whois",
      "description": "Give details about the @mentioned Symphony user",
      "helptext": "<b>/whois</b>",
      "param": "@mentioned user",
      "example": "/whois @alex nalin",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "triggers": ["UIDCheck", "uidCheck", "uidcheck", "UID", "uid"],
      "function": "UIDCheck",
      "description": "Finds details about the given UID and retrieves the Symphony user information",
      "helptext": "<b>/UIDCheck</b>",
      "param": "UID",
      "example": "/UIDCheck 71811853189765",
      "category": "Info lookupl",
      "scope": "Internal",
      "permission": "Authorised List"
    },
    {
      "triggers": ["streamCheck", "streamcheck", "stream"],
      "function": "streamCheck",
      "description": "Finds details about the given Stream and retrieves the Symphony stream information for IM/MIM/ROOMS and give detail for each",
      "helptext": "<b>/streamCheck</b>",
      "param": "streamID",
      "example": "/streamCheck 5fEubxc6pMHqioicBamPZ3___qN3RFcKdA",
      "category": "Info lookup",
      "scope": "Internal",
      "permission": "Authorised List"
    }
